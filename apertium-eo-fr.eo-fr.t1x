<?xml version="1.0" encoding="utf-8"?>

<transfer>
  <section-def-cats>
    <def-cat n="det">
      <cat-item tags="det.*"/>
    </def-cat>

    <def-cat n="nom_commun">
      <cat-item tags="n.*"/>
    </def-cat>

    <def-cat n="nom_sujet">
      <cat-item tags="n.*.nom"/>
    </def-cat>

    <def-cat n="np">
      <cat-item tags="np.*"/>
    </def-cat>

    <def-cat n="adj">
      <cat-item tags="adj.*"/>
    </def-cat>

    <def-cat n="prn">
      <cat-item tags="prn.*"/>
    </def-cat>

    <def-cat n="verbe_conj">
      <cat-item tags="vbser.pres"/>
      <cat-item tags="vblex.pres"/>
      <cat-item tags="vbtr.pres"/>
      <cat-item tags="vbntr.pres"/>
      <cat-item tags="vbtr_ntr.pres"/>
      <cat-item tags="vbser.past"/>
      <cat-item tags="vblex.past"/>
      <cat-item tags="vbtr.past"/>
      <cat-item tags="vbntr.past"/>
      <cat-item tags="vbtr_ntr.past"/>
      <cat-item tags="vbser.fti"/>
      <cat-item tags="vblex.fti"/>
      <cat-item tags="vbtr.fti"/>
      <cat-item tags="vbntr.fti"/>
      <cat-item tags="vbtr_ntr.fti"/>
    </def-cat>

    <def-cat n="etre_conj">
      <cat-item tags="vbser.pres"/>
      <cat-item tags="vbser.past"/>
      <cat-item tags="vbser.fti"/>
    </def-cat>

    <def-cat n="verbe_inf">
      <cat-item tags="vbser.inf.*"/>
      <cat-item tags="vblex.inf.*"/>
      <cat-item tags="vbtr.inf.*"/>
      <cat-item tags="vbntr.inf.*"/>
      <cat-item tags="vbtr_ntr.inf.*"/>
    </def-cat>

    <def-cat n="verbe_pp">
      <cat-item tags="vbser.pp.*"/>
      <cat-item tags="vblex.pp.*"/>
      <cat-item tags="vbtr.pp.*"/>
      <cat-item tags="vbntr.pp.*"/>
      <cat-item tags="vbtr_ntr.pp.*"/>
    </def-cat>

    <def-cat n="mot_de">
      <cat-item lemma="de" tags="pr"/>
      <cat-item lemma="de" tags="pr.*"/>
    </def-cat>

    <def-cat n="mot_la">
      <cat-item tags="det.def.*"/>
    </def-cat>

    <def-cat n="inconnu">
      <cat-item tags="cm.*"/>
    </def-cat>
  </section-def-cats>


  <section-def-attrs>
    <def-attr n="type_mot">
      <attr-item tags="n"/>
      <attr-item tags="np"/>
      <attr-item tags="det"/>
      <attr-item tags="adj"/>
      <attr-item tags="prn"/>
      <attr-item tags="vblex"/>
      <attr-item tags="vbser"/>
      <attr-item tags="vbhaver"/>
    </def-attr>

    <def-attr n="type_det">
      <attr-item tags="def"/>
      <attr-item tags="ind"/>
      <attr-item tags="pos"/>
      <attr-item tags="dem"/>
      <attr-item tags="qnt"/>
    </def-attr>

    <def-attr n="type_np">
      <attr-item tags="al"/>
      <attr-item tags="loc"/>
    </def-attr>

    <def-attr n="genre">
      <attr-item tags="m"/>
      <attr-item tags="f"/>
      <attr-item tags="mf"/>
    </def-attr>

    <def-attr n="nombre">
      <attr-item tags="sg"/>
      <attr-item tags="pl"/>
      <attr-item tags="sp"/>
    </def-attr>

    <def-attr n="personne">
      <attr-item tags="p1"/>
      <attr-item tags="p2"/>
      <attr-item tags="p3"/>
    </def-attr>

    <def-attr n="nom_acc">
      <attr-item tags="nom"/>
      <attr-item tags="acc"/>
    </def-attr>

    <def-attr n="temps">
      <attr-item tags="pres"/>
      <attr-item tags="past"/>
      <attr-item tags="pri"/>
      <attr-item tags="pii"/>
      <attr-item tags="fti"/>
    </def-attr>

    <def-attr n="posadj">
      <attr-item tags="preadj"/>
      <attr-item tags="preadj_nh"/>
    </def-attr>
  </section-def-attrs>


  <section-def-vars>
    <def-var n="genre_pp"/>
  </section-def-vars>


  <section-def-macros>
    <def-macro n="set_temps" npar="1">    <!--  concordance des temps  -->
      <choose>
        <when>
          <test>
             <equal>
                <clip pos="1" side="sl" part="temps"/>
                <lit-tag v="pres"/>
              </equal>
          </test>
          <let>
            <clip pos="1" side="tl" part="temps"/>
            <lit-tag v="pri"/>
          </let>
        </when>
        <when>
          <test>
             <equal>
                <clip pos="1" side="sl" part="temps"/>
                <lit-tag v="past"/>
              </equal>
          </test>
          <let>
            <clip pos="1" side="tl" part="temps"/>
            <lit-tag v="pii"/>
          </let>
        </when>
      </choose>
    </def-macro>

    <def-macro n="type_verbe" npar="1">    <!--  vblex, vbser etc...  -->
      <choose>
        <when>
          <test>
             <equal>
                <clip pos="1" side="sl" part="type_mot"/>
                <lit-tag v="vbser"/>
              </equal>
          </test>
          <let>
            <clip pos="1" side="tl" part="type_mot"/>
            <lit-tag v="vbser"/>
          </let>
        </when>
        <otherwise>
          <let>
            <clip pos="1" side="tl" part="type_mot"/>
            <lit-tag v="vblex"/>
          </let>
        </otherwise>
      </choose>
    </def-macro>

    <def-macro n="gen_nom_adj" npar="2">    <!--  génère nom et adjectif dans le bon ordre -->
    <!--  param1 : adjectif, param2 : nom -->
      <choose>
        <when>
          <test>
             <equal>
                <clip pos="1" side="tl" part="posadj"/>
                <lit-tag v="preadj"/>
             </equal>
          </test>
          <out>
            <lu>
              <clip pos="1" side="tl" part="lem"/>
              <clip pos="1" side="tl" part="type_mot"/>
              <clip pos="2" side="tl" part="genre"/>
              <clip pos="2" side="sl" part="nombre"/>
            </lu>
            <b />
            <lu>
              <clip pos="2" side="tl" part="lem"/>
              <clip pos="2" side="tl" part="type_mot"/>
              <clip pos="2" side="tl" part="genre"/>
              <clip pos="2" side="sl" part="nombre"/>
            </lu>
          </out>
        </when>
        <when>
          <test>
             <and>
                <equal>
                   <clip pos="1" side="tl" part="posadj"/>
                   <lit-tag v="preadj_nh"/>
                </equal>
                <not>
                   <equal>
                      <clip pos="2" side="sl" part="lem"/>
                      <lit v="viro"/>
                   </equal>
                </not>
             </and>
          </test>
          <out>
            <lu>
              <clip pos="1" side="tl" part="lem"/>
              <clip pos="1" side="tl" part="type_mot"/>
              <clip pos="2" side="tl" part="genre"/>
              <clip pos="2" side="sl" part="nombre"/>
            </lu>
            <b />
            <lu>
              <clip pos="2" side="tl" part="lem"/>
              <clip pos="2" side="tl" part="type_mot"/>
              <clip pos="2" side="tl" part="genre"/>
              <clip pos="2" side="sl" part="nombre"/>
            </lu>
          </out>
        </when>
        <otherwise>
          <out>
            <lu>
              <clip pos="2" side="tl" part="lem"/>
              <clip pos="2" side="tl" part="type_mot"/>
              <clip pos="2" side="tl" part="genre"/>
              <clip pos="2" side="sl" part="nombre"/>
            </lu>
            <b />
            <lu>
              <clip pos="1" side="tl" part="lem"/>
              <clip pos="1" side="tl" part="type_mot"/>
              <clip pos="2" side="tl" part="genre"/>
              <clip pos="2" side="sl" part="nombre"/>
            </lu>
          </out>
        </otherwise>
      </choose>
    </def-macro>

    <def-macro n="transfert_de_la" npar="1">    <!--  génère nom et adjectif dans le bon ordre -->
      <choose>
        <when>
          <test>
             <equal>
                <clip pos="1" side="sl" part="nombre"/>
                <lit-tag v="pl"/>
              </equal>
          </test>
          <out>
            <lu>
              <lit v="un"/>
              <lit-tag v="det.ind.m.pl"/>
            </lu>
          </out>
        </when>
        <when>
          <test>
             <equal>
                <clip pos="1" side="tl" part="genre"/>
                <lit-tag v="m"/>
              </equal>
          </test>
          <out>
            <lu>
              <lit v="du"/>
              <lit-tag v="pr"/>  <!-- à corriger pour éviter le # -->
            </lu>
          </out>
        </when>
        <otherwise>
          <out>
            <lu>
              <lit v="de"/>
              <lit-tag v="pr"/>
            </lu>
            <b />
            <lu>
              <lit v="le"/>
              <lit-tag v="det.def.f.sg"/>
            </lu>
          </out>
        </otherwise>
      </choose>
    </def-macro>
  </section-def-macros>


  <section-rules>
    <rule>
      <pattern>
        <pattern-item n="inconnu"/>
      </pattern>
      <action>
        <out>
          <lu>
            <clip pos="1" side="tl" part="lem"/>
            <clip pos="1" side="tl" part="type_mot"/>
          </lu>
        </out>
      </action>
    </rule>

    <rule>
      <pattern>
        <pattern-item n="det"/>
        <pattern-item n="nom_commun"/>
      </pattern>
      <action>
        <out>
          <lu>
            <clip pos="1" side="tl" part="lem"/>
            <clip pos="1" side="tl" part="type_mot"/>
            <clip pos="1" side="tl" part="type_det"/>
            <clip pos="2" side="tl" part="genre"/>
            <clip pos="2" side="sl" part="nombre"/>
          </lu>
          <b />
          <lu>
            <clip pos="2" side="tl" part="lem"/>
            <clip pos="2" side="tl" part="type_mot"/>
            <clip pos="2" side="tl" part="genre"/>
            <clip pos="2" side="sl" part="nombre"/>
          </lu>
        </out>
      </action>
    </rule>

    <rule>
      <pattern>
        <pattern-item n="nom_commun"/>
      </pattern>
      <action>
        <out>
          <lu>
            <lit v="un"/>
            <lit-tag v="det.ind"/>
            <clip pos="1" side="tl" part="genre"/>
            <clip pos="1" side="sl" part="nombre"/>
          </lu>
          <b />
          <lu>
            <clip pos="1" side="tl" part="lem"/>
            <clip pos="1" side="tl" part="type_mot"/>
            <clip pos="1" side="tl" part="genre"/>
            <clip pos="1" side="sl" part="nombre"/>
          </lu>
        </out>
      </action>
    </rule>

    <rule>
      <pattern>
        <pattern-item n="mot_de"/>
        <pattern-item n="mot_la"/>
        <pattern-item n="nom_commun"/>
      </pattern>
      <action>
        <call-macro n="transfert_de_la">
          <with-param pos="3"/>
        </call-macro>

        <out>
          <b />
          <lu>
            <clip pos="3" side="tl" part="lem"/>
            <clip pos="3" side="tl" part="type_mot"/>
            <clip pos="3" side="tl" part="genre"/>
            <clip pos="3" side="sl" part="nombre"/>
          </lu>
        </out>
      </action>
    </rule>

    <rule>
      <pattern>
        <pattern-item n="mot_de"/>
        <pattern-item n="mot_la"/>
        <pattern-item n="nom_commun"/>
        <pattern-item n="adj"/>
      </pattern>
      <action>
        <call-macro n="transfert_de_la">
          <with-param pos="3"/>
        </call-macro>

        <out>
          <b />
        </out>

        <call-macro n="gen_nom_adj">
          <with-param pos="4"/>
          <with-param pos="3"/>
        </call-macro>
      </action>
    </rule>

    <rule>
      <pattern>
        <pattern-item n="mot_de"/>
        <pattern-item n="mot_la"/>
        <pattern-item n="adj"/>
        <pattern-item n="nom_commun"/>
      </pattern>
      <action>
        <call-macro n="transfert_de_la">
          <with-param pos="4"/>
        </call-macro>

        <out>
          <b />
        </out>

        <call-macro n="gen_nom_adj">
          <with-param pos="3"/>
          <with-param pos="4"/>
        </call-macro>
      </action>
    </rule>

    <rule>
      <pattern>
        <pattern-item n="np"/>
      </pattern>
      <action>
        <out>
          <lu>
            <clip pos="1" side="tl" part="lem"/>
            <clip pos="1" side="tl" part="type_mot"/>
            <clip pos="1" side="tl" part="type_np"/>
            <clip pos="1" side="tl" part="genre"/>
            <clip pos="1" side="tl" part="nombre"/>
          </lu>
        </out>
      </action>
    </rule>

    <rule>
      <pattern>
        <pattern-item n="verbe_inf"/>
      </pattern>
      <action>
        <out>
          <lu>
            <clip pos="1" side="tl" part="lem"/>
            <clip pos="1" side="tl" part="type_mot"/>
            <lit-tag v="inf"/>
          </lu>
        </out>
      </action>
    </rule>

    <rule>
      <pattern>
        <pattern-item n="verbe_pp"/>
      </pattern>
      <action>
        <out>
          <lu>
            <clip pos="1" side="tl" part="lem"/>
            <clip pos="1" side="tl" part="type_mot"/>
            <lit-tag v="pp.m"/>
            <clip pos="1" side="tl" part="nombre"/>
          </lu>
        </out>
      </action>
    </rule>

    <rule>
      <pattern>
        <pattern-item n="prn"/>
        <pattern-item n="verbe_conj"/>
      </pattern>

      <action>

        <choose>    <!--  cas particuliers de transferts des pronoms  -->
          <when>    <!--  2ème personne toujours au pluriel : vi -> vous  -->
            <test>
               <equal>
                  <clip pos="1" side="sl" part="personne"/>
                  <lit-tag v="p2"/>
                </equal>
            </test>
            <let>
              <clip pos="1" side="tl" part="nombre"/>
              <lit-tag v="pl"/>
            </let>
          </when>
          <when>    <!--  3ème personne du pluriel toujours au masculin : ili -> ils  -->
            <test>
               <and>
                 <equal>
                    <clip pos="1" side="sl" part="personne"/>
                    <lit-tag v="p3"/>
                  </equal>
                 <equal>
                    <clip pos="1" side="sl" part="nombre"/>
                    <lit-tag v="pl"/>
                  </equal>
                </and>
            </test>
            <let>
              <clip pos="1" side="tl" part="genre"/>
              <lit-tag v="m"/>
            </let>
          </when>
        </choose>

        <call-macro n="set_temps">
          <with-param pos="2"/>
        </call-macro>

        <call-macro n="type_verbe">
          <with-param pos="2"/>
        </call-macro>

        <out>
          <lu>
            <clip pos="1" side="tl" part="lem"/>
            <clip pos="1" side="tl" part="type_mot"/>
            <clip pos="1" side="tl" part="personne"/>
            <clip pos="1" side="tl" part="genre"/>
            <clip pos="1" side="tl" part="nombre"/>
          </lu>
          <b />
          <lu>
            <clip pos="2" side="tl" part="lem"/>
            <clip pos="2" side="tl" part="type_mot"/>
            <clip pos="2" side="tl" part="temps"/>
            <clip pos="1" side="tl" part="personne"/>
            <clip pos="1" side="tl" part="nombre"/>
          </lu>
        </out>
      </action>
    </rule>

    <rule>
      <pattern>
        <pattern-item n="prn"/>
        <pattern-item n="etre_conj"/>
        <pattern-item n="verbe_pp"/>
      </pattern>

      <action>

        <choose>    <!--  cas particuliers de transferts des pronoms  -->
          <when>    <!--  2ème personne toujours au pluriel : vi -> vous  -->
            <test>
               <equal>
                  <clip pos="1" side="sl" part="personne"/>
                  <lit-tag v="p2"/>
                </equal>
            </test>
            <let>
              <clip pos="1" side="tl" part="nombre"/>
              <lit-tag v="pl"/>
            </let>
          </when>
          <when>    <!--  3ème personne du pluriel toujours au masculin : ili -> ils  -->
            <test>
               <and>
                 <equal>
                    <clip pos="1" side="sl" part="personne"/>
                    <lit-tag v="p3"/>
                  </equal>
                 <equal>
                    <clip pos="1" side="sl" part="nombre"/>
                    <lit-tag v="pl"/>
                  </equal>
                </and>
            </test>
            <let>
              <clip pos="1" side="tl" part="genre"/>
              <lit-tag v="m"/>
            </let>
          </when>
        </choose>

        <choose>    <!--  si le genre du pronom est mf, celui du participe passé sera m  -->
          <when>
            <test>
               <equal>
                  <clip pos="1" side="tl" part="genre"/>
                  <lit-tag v="mf"/>
                </equal>
            </test>
            <let>
              <var n="genre_pp"/>
              <lit-tag v="m"/>
            </let>
          </when>
          <otherwise>
            <let>
              <var n="genre_pp"/>
              <clip pos="1" side="tl" part="genre"/>
            </let>
          </otherwise>
        </choose>

        <call-macro n="set_temps">
          <with-param pos="2"/>
        </call-macro>

        <out>
          <lu>
            <clip pos="1" side="tl" part="lem"/>
            <clip pos="1" side="tl" part="type_mot"/>
            <clip pos="1" side="tl" part="personne"/>
            <clip pos="1" side="tl" part="genre"/>
            <clip pos="1" side="tl" part="nombre"/>
          </lu>
          <b />
          <lu>
            <clip pos="2" side="tl" part="lem"/>
            <lit-tag v="vbser"/>
            <clip pos="2" side="tl" part="temps"/>
            <clip pos="1" side="tl" part="personne"/>
            <clip pos="1" side="tl" part="nombre"/>
          </lu>
          <b />
          <lu>
            <clip pos="3" side="tl" part="lem"/>
            <clip pos="3" side="tl" part="type_mot"/>
            <lit-tag v="pp"/>
            <var n="genre_pp"/>
            <clip pos="1" side="tl" part="nombre"/>
          </lu>
        </out>
      </action>
    </rule>

    <rule>
      <pattern>
        <pattern-item n="det"/>
        <pattern-item n="nom_sujet"/>
        <pattern-item n="verbe_conj"/>
      </pattern>

      <action>
        <call-macro n="set_temps">
          <with-param pos="3"/>
        </call-macro>

       <call-macro n="type_verbe">
          <with-param pos="3"/>
        </call-macro>

        <out>
          <lu>
            <clip pos="1" side="tl" part="lem"/>
            <clip pos="1" side="tl" part="type_mot"/>
            <clip pos="1" side="tl" part="type_det"/>
            <clip pos="2" side="tl" part="genre"/>
            <clip pos="2" side="sl" part="nombre"/>
          </lu>
          <b />
          <lu>
            <clip pos="2" side="tl" part="lem"/>
            <clip pos="2" side="tl" part="type_mot"/>
            <clip pos="2" side="tl" part="genre"/>
            <clip pos="2" side="sl" part="nombre"/>
          </lu>
          <b />
          <lu>
            <clip pos="3" side="tl" part="lem"/>
            <clip pos="3" side="tl" part="type_mot"/>
            <clip pos="3" side="tl" part="temps"/>
            <lit-tag v="p3"/>
            <clip pos="2" side="sl" part="nombre"/>
          </lu>
        </out>
      </action>
    </rule>

    <rule>
      <pattern>
        <pattern-item n="nom_sujet"/>
        <pattern-item n="verbe_conj"/>
      </pattern>

      <action>
        <call-macro n="set_temps">
          <with-param pos="2"/>
        </call-macro>

        <call-macro n="type_verbe">
          <with-param pos="2"/>
        </call-macro>

        <out>
          <lu>
            <lit v="un"/>
            <lit-tag v="det.ind"/>
            <clip pos="1" side="tl" part="genre"/>
            <clip pos="1" side="sl" part="nombre"/>
          </lu>
          <b />
          <lu>
            <clip pos="1" side="tl" part="lem"/>
            <clip pos="1" side="tl" part="type_mot"/>
            <clip pos="1" side="tl" part="genre"/>
            <clip pos="1" side="sl" part="nombre"/>
          </lu>
          <b />
          <lu>
            <clip pos="2" side="tl" part="lem"/>
            <clip pos="2" side="tl" part="type_mot"/>
            <clip pos="2" side="tl" part="temps"/>
            <lit-tag v="p3"/>
            <clip pos="1" side="sl" part="nombre"/>
          </lu>
        </out>
      </action>
    </rule>

    <rule>
      <pattern>
        <pattern-item n="det"/>
        <pattern-item n="nom_sujet"/>
        <pattern-item n="verbe_pp"/>
      </pattern>

      <action>
        <out>
          <lu>
            <clip pos="1" side="tl" part="lem"/>
            <clip pos="1" side="tl" part="type_mot"/>
            <clip pos="1" side="tl" part="type_det"/>
            <clip pos="2" side="tl" part="genre"/>
            <clip pos="2" side="sl" part="nombre"/>
          </lu>
          <b />
          <lu>
            <clip pos="2" side="tl" part="lem"/>
            <clip pos="2" side="tl" part="type_mot"/>
            <clip pos="2" side="tl" part="genre"/>
            <clip pos="2" side="sl" part="nombre"/>
          </lu>
          <b />
          <lu>
            <clip pos="3" side="tl" part="lem"/>
            <clip pos="3" side="tl" part="type_mot"/>
            <lit-tag v="pp"/>
            <clip pos="2" side="tl" part="genre"/>
            <clip pos="2" side="sl" part="nombre"/>
          </lu>
        </out>
      </action>
    </rule>

    <rule>
      <pattern>
        <pattern-item n="nom_sujet"/>
        <pattern-item n="verbe_pp"/>
      </pattern>

      <action>
        <out>
          <lu>
            <lit v="un"/>
            <lit-tag v="det.ind"/>
            <clip pos="1" side="tl" part="genre"/>
            <clip pos="1" side="sl" part="nombre"/>
          </lu>
          <b />
          <lu>
            <clip pos="1" side="tl" part="lem"/>
            <clip pos="1" side="tl" part="type_mot"/>
            <clip pos="1" side="tl" part="genre"/>
            <clip pos="1" side="sl" part="nombre"/>
          </lu>
          <b />
          <lu>
            <clip pos="2" side="tl" part="lem"/>
            <clip pos="2" side="tl" part="type_mot"/>
            <lit-tag v="pp"/>
            <clip pos="1" side="tl" part="genre"/>
            <clip pos="1" side="sl" part="nombre"/>
          </lu>
        </out>
      </action>
    </rule>

    <rule>
      <pattern>
        <pattern-item n="det"/>
        <pattern-item n="nom_sujet"/>
        <pattern-item n="etre_conj"/>
        <pattern-item n="verbe_pp"/>
      </pattern>

      <action>
        <call-macro n="set_temps">
          <with-param pos="3"/>
        </call-macro>

        <out>
